RUN: rm -rf %t
RUN: mkdir -p %t/home %t/repo
RUN: cd %t/repo
RUN: git init -b main
RUN: git config user.name gitkey-test
RUN: git config user.email gitkey-test@example.invalid
RUN: echo "base" > README.txt
RUN: git add README.txt
RUN: git commit -m "Base commit"
RUN: git checkout -b feature
RUN: echo "feature change" > README.txt
RUN: git add README.txt
RUN: git commit -m "Feature commit"
RUN: git checkout main
RUN: echo "main change" > README.txt
RUN: git add README.txt
RUN: git commit -m "Main commit"
RUN: git checkout feature
RUN: git rebase main || true
RUN: echo "resolved" > README.txt
RUN: git add README.txt
RUN: %expect_exit 0 bash -c "cd %t/repo && printf '\033[1;2C' | env %gitkey_env GIT_EDITOR=true $GITKEY_BIN | filecheck %s"

CHECK: git rebase --continue
CHECK: git status
