RUN: rm -rf %t
RUN: mkdir -p %t/home %t/repo %t/bin
RUN: cd %t/repo
RUN: git init -b main
RUN: git config user.name gitkey-test
RUN: git config user.email gitkey-test@example.invalid
RUN: echo "fixture" > README.txt
RUN: git add README.txt
RUN: git commit -m "Initial commit"
RUN: git checkout -b feature
RUN: git checkout main
RUN: printf '#!/bin/sh\necho feature\n' > %t/bin/%copy_and_paste_tool
RUN: chmod +x %t/bin/%copy_and_paste_tool
RUN: %expect_exit 0 bash -c "cd %t/repo && printf 'O' | env %gitkey_env PATH=%t/bin:$PATH $GITKEY_BIN | filecheck %s"

CHECK: git rev-parse --verify feature
CHECK: git checkout feature
