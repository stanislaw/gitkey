RUN: rm -rf %t
RUN: mkdir -p %t/home %t/repo
RUN: cd %t/repo
RUN: git init -b main
RUN: git config user.name gitkey-test
RUN: git config user.email gitkey-test@example.invalid
RUN: echo "fixture" > README.txt
RUN: git add README.txt
RUN: git commit -m "Initial commit"
RUN: %expect_exit 0 bash -c "cd %t/repo && printf 'Nfeature\n' | env %gitkey_env USER=tester $GITKEY_BIN | filecheck %s"

CHECK: git switch -c tester/feature
