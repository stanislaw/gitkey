RUN: rm -rf %t
RUN: mkdir -p %t/home %t/repo
RUN: cd %t/repo && git init -b main
RUN: cd %t/repo && git config user.name gitkey-test
RUN: cd %t/repo && git config user.email gitkey-test@example.invalid
RUN: cd %t/repo && echo "first" > README.txt
RUN: cd %t/repo && git add README.txt
RUN: cd %t/repo && git commit -m "First commit"
RUN: cd %t/repo && echo "second" >> README.txt
RUN: cd %t/repo && git add README.txt
RUN: cd %t/repo && git commit -m "Second commit"
RUN: %expect_exit 0 bash -c "cd %t/repo && printf '9' | $GITKEY_BIN | filecheck %s"

CHECK: git reset HEAD^
CHECK: git status
